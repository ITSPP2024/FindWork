modules = ["nodejs-20", "python-3.11", "postgresql-16"]

[nix]
channel = "stable-25_05"

[agent]
expertMode = true
integrations = ["replitmail:1.0.0", "twilio_send_message:1.0.0", "javascript_database:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "FindWork Application"

[[workflows.workflow]]
name = "FindWork Application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm start"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 36103
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["npm", "start"]
